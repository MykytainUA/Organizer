package shared.messages;

/**
 * Types of response
 */
public enum ResponseState {
	
	UNKNOWN {
		@Override
		public String getAsString() {
			return "UNKNOWN";
		}
	},
	
	SUCCESSFULL {
		@Override
		public String getAsString() {
			return "SUCCESSFULL";
		}
	}, 
	
	NO_SUCH_ENTRY_IN_DATABASE {
		@Override
		public String getAsString() {
			return "NO_SUCH_ENTRY_IN_DATABASE";
		}
	}, 
	
	ERROR {
		@Override
		public String getAsString() {
			return "ERROR";
		}
	}; 
	
	public String getAsString() {
		return "UNKNOWN";
	}
}
